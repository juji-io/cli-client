#!/usr/bin/env NODE_NO_WARNINGS=1 node

var program = require('commander');

program
    .version('0.1.0')
    .description('Simple Juji client')
    .command('chat <url>', 'chat with a Juji bot')
    .command('login', 'login to Juji')
    .command('logout', 'logout from Juji')
    .command('upload', 'upload data to Juji for analysis')
    .command('query', 'issue GraphQL query to Juji')
    .parse(process.argv);
